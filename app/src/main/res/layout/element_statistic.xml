<?xml version="1.0" encoding="utf-8"?>

<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <data>

        <import type="android.view.View" />

        <variable
            name="context"
            type="android.content.Context" />

        <import type="androidx.core.content.ContextCompat" />

        <variable
            name="statistic"
            type="com.bunbeauty.fooddeliveryadmin.data.model.Statistic" />

        <variable
            name="iStringHelper"
            type="com.bunbeauty.fooddeliveryadmin.utils.string.IStringHelper" />
    </data>

    <com.google.android.material.card.MaterialCardView
        android:id="@+id/element_order_mvc_main"
        android:padding="@dimen/small_padding"
        style="@style/DefaultMaterialCard">

            <TextView
                style="@style/MainTitle"
                android:layout_margin="@dimen/small_margin"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@{statistic.date}"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />
    </com.google.android.material.card.MaterialCardView>
</layout>